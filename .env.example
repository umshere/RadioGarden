# AI provider selection
AI_PROVIDER=openrouter
USE_MOCK=false

# Provider credentials
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4o-mini
OPENROUTER_API_KEY=your_openrouter_api_key_here
OPENROUTER_MODEL=meta-llama/llama-3.3-8b-instruct:free
OLLAMA_URL=http://localhost:11434
OLLAMA_MODEL=radio-passport

# Enable feature previews
# Experimental Raptor mini compact player; default disabled locally. When set to
# `true`, the app renders the new compact 'Raptor' mini player variant on mobile
# as a preview for all clients.
ENABLE_RAPTOR_MINI=false
